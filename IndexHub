local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Index Hub", HidePremium = false, IntroEnabled = true, IntroText = "Index Hub", IntroIcon = "http://www.roblox.com/asset/?id=951528747", SaveConfig = true, ConfigFolder = "IndexTest"})

OrionLib:MakeNotification({
	Name = "Index Hub",
	Content = "Made By Someone",
	Image = "rbxassetid://4483345998",
	Time = 5
})

