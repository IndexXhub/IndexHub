----//Main//----
local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Player Main"
})

Tab:AddParagraph("Index Hub","Everything is Tested Using Solara")
Tab:AddLabel("Thanks For Using Index Hub")

Tab:AddButton({
	Name = "UNC TEST",
	Callback = function()
      	loadstring(game:HttpGet("https://github.com/ltseverydayyou/uuuuuuu/blob/main/UNC%20test?raw=true"))()
  	end    
})

local UserInputService = game:GetService("UserInputService")
local Toggle = false

local function toggleInfiniteJump(enabled)
    Toggle = enabled
end

local function performInfiniteJump()
    if Toggle then
        game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
    end
end

toggleInfiniteJump(false)

Tab:AddToggle({
    Name = "INF JUMP",
    Default = false,
    Callback = function(Value)
        toggleInfiniteJump(Value)
    end    
})

UserInputService.JumpRequest:Connect(function()
    performInfiniteJump()
end)

Tab:AddSlider({
	Name = "WalkSpeed Changer",
	Min = 16,
	Max = 500,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "WalkSpeed",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = (Value)
	end    
})

Tab:AddSlider({
	Name = "JumpPower Changer",
	Min = 50,
	Max = 500,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "JumpPower",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = (Value)
	end    
})

local Workspace = game.Workspace

local function changeGravity(newGravity)
    Workspace.Gravity = newGravity
end

changeGravity(196.2)

Tab:AddSlider({
    Name = "Gravity Slider",
    Min = 0,
    Max = 500,
    Default = 196.2,
    Color = Color3.fromRGB(255, 255, 255),
    Increment = 1,
    ValueName = "Gravity",
    Callback = function(Value)
        changeGravity(Value)
    end    
})

Tab:AddButton({
	Name = "Gravity Reset",
	Callback = function()
		changeGravity(196.2)
  	end    
})	

local Section = Tab:AddSection({
	Name = "Other Player"
})

local player = game.Players.LocalPlayer
local camera = game.Workspace.CurrentCamera

local function changeFOV(newFOV)
    camera.FieldOfView = newFOV
end

changeFOV(70)

Tab:AddSlider({
	Name = "FOV Changer",
	Min = 0,
	Max = 120,
	Default = 70,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "FOV",
	Callback = function(Value)
		changeFOV(Value)
	end    
})

Tab:AddButton({
	Name = "FOV Reset",
	Callback = function()
		changeFOV(70)
  	end    
})	

local player = game.Players.LocalPlayer

local function changeZoomLimits(minZoom, maxZoom)
    player.CameraMinZoomDistance = minZoom
    player.CameraMaxZoomDistance = maxZoom
end

local initialMinZoom = 10
local initialMaxZoom = 50
changeZoomLimits(initialMinZoom, initialMaxZoom)

Tab:AddSlider({
    Name = "Max Zoom Changer",
    Min = 50,
    Max = 2000000,
    Default = initialMaxZoom,
    Color = Color3.fromRGB(255, 255, 255),
    Increment = 1,
    ValueName = "Zoom",
    Callback = function(Value)
        changeZoomLimits(initialMinZoom, Value)
    end    
})

Tab:AddButton({
	Name = "Destroy GUI",
	Callback = function()
		OrionLib:Destroy()
  	end    
})	

